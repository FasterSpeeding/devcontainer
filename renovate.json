{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>FasterSpeeding/Piped//config/renovate.json"
  ],
  "mise": {
    "managerFilePatterns": ["/^artifacts/\\.config/mise/config\\.toml$/"]
  },
  "packageRules": [
    {
      "matchFileNames": [
        ".devcontainer.json"
      ],
      "enabled": false
    },
    {
      "matchFileNames": ["renovate.json"],
      "matchPackageNames": ["java-jdk"],
      "replacementVersionTemplate": "{{ (split currentVallue '+') }}"
    }
  ]
}
